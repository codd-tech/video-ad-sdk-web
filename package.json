{
  "name": "@codd-tech/video-ad-sdk",
  "description": "Web Advertising Platform SDK",
  "repository": "https://github.com/codd-tech/video-ad-sdk-web",
  "homepage": "https://github.com/codd-tech/video-ad-sdk-web",
  "author": "n.kupreev@codd.tech",
  "version": "0.2.4",
  "keywords": [
    "advertisement",
    "advertisement sdk",
    "advertisement web",
    "advertisement telegram"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "pretty": "prettier --write \"./**/*.{ts,tsx,json,css,html}\"",
    "prepare": "husky",
    "lint-staged": "lint-staged --config ./lint-staged.config.cjs",
    "ts-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.23.0",
    "@kinescope/player-iframe-api-loader": "^0.1.2",
    "@tanstack/react-query": "^5.71.5",
    "antd": "^5.24.6",
    "compose-function": "^3.0.3",
    "ky": "^1.8.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@tanstack/eslint-plugin-query": "^5.71.5",
    "@types/compose-function": "^0.0.33",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@types/telegram-web-app": "^7.10.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.23.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.86.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.4",
    "vite-plugin-dts": "^4.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "yarn@4.7.0"
}
